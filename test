WORKDIR /app

COPY requirements.txt .

RUN pip install --no-cache-dir -r requirements.txt

COPY .env .env  # Ignore .env file

COPY . .

CMD ["python", "main.py"]



FROM python:3.12-bullseye
WORKDIR /app
COPY requirements.txt .
RUN pip install --no-cache-dir -r requirements.txt
COPY . .
ENV DISCORD_TOKEN=""
CMD ["python3", "main.py"]